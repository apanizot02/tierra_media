n\n\n    \n  </div>\n\n  <footer class=\"card-footer\">\n    <span>1 Action</span>\n    <span>1 Creature</span>\n    <span>30 Feet</span>\n    <span>Instantaneous</span>\n  </footer>\n</div>","speaker":{"scene":"MFYvEUqniTHNtc7G","token":"gVcZVKQH1SMgaYdD","actor":"ar0SDpFo73QPIDSH","alias":"Ragnok el Emisario de la Muerte"},"whisper":[],"blind":false,"emote":false,"flags":{"midi-qol":{"itemUuid":"Actor.ar0SDpFo73QPIDSH.Item.oUNqRGtZrFHXlRGy","actorUuid":"Actor.ar0SDpFo73QPIDSH","sound":"","type":0,"itemId":"oUNqRGtZrFHXlRGy","workflowId":"Actor.ar0SDpFo73QPIDSH.Item.oUNqRGtZrFHXlRGy"},"core":{"canPopout":true}}}
